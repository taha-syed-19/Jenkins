pipeline {
    agent any
    
    stages {
        stage('Stage Built') {
            steps {
                echo 'Hello, Jenkins!'
            }
        }
        
        stage('Test') {
            steps {
                echo 'Testing on the way!'
            }
        }
        
        stage('Finish') {
            steps {
                echo 'Finishes Testing!'
            }
        }
    }
}
